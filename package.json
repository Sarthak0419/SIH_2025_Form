{
  "name": "sih-2025-form",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8080 --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "build:production": "NODE_ENV=production tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview --port 8080 --host 0.0.0.0",
    "backend": "npx tsx src/services/server.ts",
    "backend:dev": "npx tsx watch src/services/server.ts",
    "backend:build": "tsc src/services/server.ts --outDir dist --target es2020 --module commonjs",
    "start": "concurrently \"npm run dev\" \"npm run backend:dev\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@vercel/analytics": "^1.5.0",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "mongodb": "^6.18.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.2.1",
    "@types/nodemailer": "^7.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^8.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "terser": "^5.43.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
